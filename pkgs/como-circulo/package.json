{
  "name": "@solose-ts/como-circulo",
  "version": "0.1.1",
  "packageManager": "pnpm@10.15.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    }
  },
  "scripts": {
    "prepare": "tshy",
    "clean": "rimraf dist",
    "test": "pnpm vitest run",
    "lint": "pnpm --fix eslint .",
    "lint:check": "pnpm eslint .",
    "fmt": "prettier --cache --write \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,md,json}\"",
    "fmt:check": "prettier --cache --check \"**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,md,json}\""
  },
  "dependencies": {
    "canonical-json": "^0.2.0",
    "dedent": "^1.7.0",
    "nanoid": "^5.1.5",
    "nochoices": "^1.1.5",
    "snarkjs": "^0.7.5",
    "tempy": "^3.1.0"
  },
  "peerDependencies": {
    "vitest": ">3.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^24.3.3",
    "@types/snarkjs": "^0.7.9",
    "esbuild": "^0.25.10",
    "esbuild-node-externals": "^1.18.0",
    "eslint": "^9.36.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "tshy": "^3.0.2",
    "typescript": "^5",
    "typescript-eslint": "^8.44.1",
    "vitest": "^3.2.0"
  },
  "tshy": {
    "exports": {
      "./package.json": "./package.json",
      ".": "./src/index.ts"
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js"
}
